biasJaffe$X.txt: jaffe.dat
	gsprealize$x gsp=jaffe.dat out=- randrad=false nbody=2m seed=54321 copies=1 | \
	  snapstat - value="- m / rsqrt(x*x + y*y + z*z + 0.0025*0.0025)" options=sum
	gsprealize$X gsp=jaffe.dat out=- randrad=true nbody=2m seed=54321 copies=128  | \
	  snapstat - value="- m / rsqrt(x*x + y*y + z*z + 0.0025*0.0025) + 4.0431/n" \
	           options=sum > biasJaffe$X.txt

jaffe.dat:
	gammagsp - 2.0 mtot=0.0625 a=0.04 rrange=1/4194304:32 npoint=865 | \
	  gsptrun - jaffe.dat rtrun=4.0
