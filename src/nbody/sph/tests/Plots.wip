define limMinMax
  xcol 1
  ycol $1
  limits
  set \0 y1
  ycol $2
  limits
  limits x1 x2 \0 y2
end

define limMinMaxLog
  xcol 1
  ycol $1
  log y
  limits
  set \0 y1
  ycol $2
  log y
  limits
  limits x1 x2 \0 y2
end

define plotRad
  data r$1/r$1_rad.txt
  limMinMaxLog 3 9
  box BCNST BCNST
  xlabel t
  ylabel log r
  xcol 1
  ycol 3
  log y
  conn
  ycol 4
  log y
  conn
  ycol 5
  log y
  conn
  ycol 6
  log y
  conn
  ycol 7
  log y
  conn
  ycol 8
  log y
  conn
  ycol 9
  log y
  conn
  ycol 10
  log y
  lstyle 4
  conn
  lstyle 1
end

define plotEnergy
  data r$1/r$1_energy.txt
  limMinMax 5 6
  set \0 (-1 * y1)
